{
  "name": "@nemi-fi/wallet-sdk",
  "version": "0.85.0-obsidion.1",
  "type": "module",
  "main": "src/exports/index.ts",
  "module": "src/exports/index.ts",
  "exports": {
    ".": {
      "import": "./src/exports/index.ts"
    },
    "./*": {
      "import": "./src/exports/*.ts"
    }
  },
  "publishConfig": {
    "main": "dist/exports/index.js",
    "module": "dist/exports/index.js",
    "types": "dist/exports/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/exports/index.js",
        "types": "./dist/exports/index.d.ts"
      },
      "./*": {
        "import": "./dist/exports/*.js",
        "types": "./dist/exports/*.d.ts"
      }
    },
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "pnpm _chore && tsc -w",
    "build": "pnpm _chore && rm -rf dist && tsc",
    "test": "pnpm test:lint && pnpm test:unit",
    "test:unit": "vitest run",
    "test:lint": "tsc --noEmit --emitDeclarationOnly false && prettier --check .",
    "test:lint:fix": "prettier --write .",
    "prepublishOnly": "pnpm test && pnpm build",
    "_chore": "pnpm i"
  },
  "devDependencies": {
    "@aztec/accounts": "0.85.0",
    "@aztec/noir-contracts.js": "0.85.0",
    "@obsidion/bridge": "0.4.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.11",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@aztec/aztec.js": "0.85.0",
    "@aztec/constants": "0.85.0",
    "@aztec/entrypoints": "0.85.0",
    "@aztec/protocol-contracts": "0.85.0",
    "@aztec/stdlib": "0.85.0",
    "@walletconnect/modal-sign-html": "^2.6.2",
    "@walletconnect/universal-provider": "^2.18.0",
    "@walletconnect/utils": "^2.11.2",
    "lodash-es": "^4.17.21",
    "ox": "^0.6.9",
    "svelte": "^5.17.3",
    "svelte-persisted-store": "^0.11.0",
    "svelte-reactive": "^0.0.10",
    "ts-essentials": "^9.4.1",
    "ufo": "^1.5.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
